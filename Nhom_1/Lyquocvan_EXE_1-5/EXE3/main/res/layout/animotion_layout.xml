<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/nennnnnn"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/saobang"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:src="@drawable/saobangg" />

        <ImageView
            android:id="@+id/saobang01"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginLeft="200dp"
            android:src="@drawable/saobangg" />

        <ImageView
            android:id="@+id/saobang02"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginLeft="300dp"
            android:src="@drawable/saobangg" />

        <ImageView
            android:id="@+id/starb"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="300dp"
            android:layout_marginTop="-100dp"
            android:src="@drawable/starb" />

        <ImageView
            android:id="@+id/sao02"
            android:layout_width="5dp"
            android:layout_height="5dp"
            android:layout_marginLeft="150dp"
            android:src="@drawable/untitled1" />

        <ImageView
            android:id="@+id/sao03"
            android:layout_width="5dp"
            android:layout_height="5dp"
            android:src="@drawable/untitled1" />

        <ImageView
            android:id="@+id/traidat"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginLeft="45dp"
            android:src="@drawable/traidat" />

        <ImageView
            android:id="@+id/sao01"
            android:layout_width="5dp"
            android:layout_height="5dp"
            android:layout_marginLeft="80dp"
            android:src="@drawable/untitled1" />

        <ImageView
            android:id="@+id/stara"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:src="@drawable/stara" />

        <ImageView
            android:id="@+id/sao"
            android:layout_width="5dp"
            android:layout_height="5dp"
            android:layout_marginLeft="100dp"
            android:src="@drawable/untitled1" />

        <ImageView
            android:id="@+id/diabay"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="-200dp"
            android:src="@drawable/diabay" />

        <ImageView
            android:id="@+id/phithuyen"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="-60dp"
            android:src="@drawable/phithuyen" />

        <ImageView
            android:id="@+id/tenlua"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="170dp"
            android:src="@drawable/tenlua" />
    </LinearLayout>
</LinearLayout>